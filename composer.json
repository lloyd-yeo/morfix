{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "aweber/aweber": "^1.1",
        "braintree/braintree_php": "^3.25",
        "emojione/emojione": "^2.2",
        "guzzlehttp/guzzle": "^6.2",
        "intervention/image": "^2.3",
        "laravel-notification-channels/telegram": "^0.0.3",
        "laravel/cashier": "^7.0",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4",
        "lazyjsonmapper/lazyjsonmapper": "^1.2",
        "lloydyeo/instagram-api": "dev-master",
        "paypal/rest-api-sdk-php": "*",
        "riverskies/laravel-mobile-detect": "^1.3",
        "sendgrid/sendgrid": "^6.0",
        "unicodeveloper/laravel-emoji": "^1.0",
        "webpatser/laravel-uuid": "2.*",
        "xethron/migrations-generator": "^2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "InstagramAPI\\": "vendor/lloydyeo/instagram-php/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
